<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="307.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainFormController">
   <children>
      <TextField fx:id="txtLanguage" layoutX="14.0" layoutY="21.0">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor>
      </TextField>
      <TextField fx:id="txtSelectedLanguage" layoutX="14.0" layoutY="346.0">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </TextField>
      <Button layoutX="222.0" layoutY="21.0" mnemonicParsing="false" onAction="#saveLanguageOnAction" text="Save" />
      <ListView fx:id="lstLanguage" layoutX="14.0" layoutY="68.0" prefHeight="260.0" prefWidth="200.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ListView>
   </children>
</AnchorPane>
